# Discord Bot Configuration
DISCORD_TOKEN=YOUR_BOT_TOKEN_HERE
CLIENT_ID=YOUR_CLIENT_ID_HERE
GUILD_ID=YOUR_GUILD_ID_HERE

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/rrpbot
DATABASE_NAME=rrpbot

# Bot Settings
PREFIX=!
LOG_CHANNEL_ID=your_log_channel_id
SUPPORT_ROLE_ID=your_support_role_id

# Music Configuration
MUSIC_MAX_QUEUE_SIZE=100
MUSIC_DEFAULT_VOLUME=50

# Moderation Settings
AUTO_MOD_ENABLED=true
SPAM_DETECTION_ENABLED=true

# Ticket System - Staff Log Channel
STAFF_LOG_CHANNEL_ID=your_staff_log_channel_id

# Environment
NODE_ENV=development

